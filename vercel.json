{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_BASE_URL": "/api",
    "VITE_TENANT_NAME": "wojo"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "/api",
      "VITE_TENANT_NAME": "wojo"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://103.54.170.35:8001/api/v1/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
